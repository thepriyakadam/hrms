<table  id="example1" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Employee Code</th>
      <th>Employee Name</th>
      <th>Hrms Access Role</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @employees.each do |m| %>   
        <tr>
          <td><%= m.manual_member_code %></td>
          <td><%= m.employee.first_name %> <%= m.employee.middle_name %> <%= m.employee.last_name %></td> 
          <td><%= m.role.try(:name) %>
          <td><%= link_to "", edit_role_role_path(m), class: "btn btn-success btn-xs fa fa-edit", remote:true %></td>
        </tr> 
    <% end %>
  </tbody>
</table>