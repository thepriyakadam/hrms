<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Period</th>
      <th>Employee</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
     <% @goal_bunches.each do |goal_bunch| %>
      <tr>
        <td><%= goal_bunch.period %></td>
        <td><%= goal_bunch.employee %></td>
        <td>
          <%= link_to '', edit_goal_bunch_path(goal_bunch),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
          <%= link_to '',  goal_bunch, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-xs btn-danger fa fa-close' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
