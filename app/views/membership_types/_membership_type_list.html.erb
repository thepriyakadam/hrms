<br>
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Code</th>
      <th>Member Type</th>
      <th>Status</th>
      <th>Action</th>
   </tr>
   </thead>
  <tbody>
    <% @membership_types.each do |membership_type| %>
      <tr>
        <td><%= membership_type.code %></td>
        <td><%= membership_type.name %></td>
         <td><%= membership_type.status ? "Active" : "Inactive" %></td>
        <td>
            <%= link_to '', edit_membership_type_path(membership_type),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
            <%= link_to '',  membership_type, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger btn-xs fa fa-close',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>