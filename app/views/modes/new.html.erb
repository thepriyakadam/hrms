
<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Mode</h3>
      <div class="pull-right">
    <%= link_to " PDF ", mode_detail_modes_path(id: @mode.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", mode_detail_modes_path(id: @mode.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to " Upload ", import_xl_modes_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template ", "/Templates/68.Mode.xls", class: "fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <div id="form">
      <%= render 'form' %>  
    </div>
    
    <div id="mode_list">
      <%= render 'mode_list' %>
    </div>
    
  </div><!-- /.box-body -->
</div><!-- /.box -->
