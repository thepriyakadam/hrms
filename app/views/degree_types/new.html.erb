<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Qualification Level</h3>
    <div class="pull-right">
    <%= link_to " PDF ", qualification_level_master_degree_types_path(id: @degree_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", qualification_level_master_degree_types_path(id: @degree_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
    
    <%= link_to "Upload Qualification Level", import_xl_degree_types_path, class: 'btn btn-xs btn-primary' %>
    
     <%= link_to "Template", "/Templates/38.Qualification Level.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="degree_type_li">
			<%= render 'degree_type_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->