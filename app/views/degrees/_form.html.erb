<%= bootstrap_form_for(@degree,html: {id: 'degree'},remote:true) do |f| %>
  <div class="row">
    <div class="col-sm-3">
      <div class="form-group required">
        <%= f.text_field :name,hide_label:true, placeholder: 'Qualification' %>
      </div>
    </div>

    <div class="col-sm-3">
      <div class="actions">
      <%= f.submit "Create Qualification",:class => class_name, data: { disable_with: data_info } %>
      </div>
    </div>
  </div>
<% end %>
