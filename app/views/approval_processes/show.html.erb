<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Approval Process</h3>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover table-condensed">

    <tr>
    	<th>Member</th>
    	<td><%= full_name(@approval_process.membership.employee)%></td>
    	<th>From</th>
    	<td><%= @approval_process.from %></td>

    </tr>

    <tr>
    	<th>To</th>
    	<td><%= @approval_process.to %></td>

    	<th>Status</th>
    	<td><%= @approval_process.status ? "Active" : "Inactive" %></td>
    </tr>


</table><br>
<%= link_to 'Edit', edit_approval_process_path(@approval_process) ,class:'btn btn-sm btn-success fa fa-edit'%> |
<%= link_to 'Back', new_approval_process_path,class:'btn btn-sm btn-default fa fa-arrow-left' %>
</div>
</div>