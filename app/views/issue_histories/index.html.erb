<p id="notice"><%= notice %></p>

<h1>Listing Issue Histories</h1>

<table>
  <thead>
    <tr>
      <th>Issue request</th>
      <th>Issue master</th>
      <th>Description</th>
      <th>Date</th>
      <th>Time</th>
      <th>Employee</th>
      <th>Issue tracker member</th>
      <th>Issue tracker group</th>
      <th>Issue priority</th>
      <th>Status</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_histories.each do |issue_history| %>
      <tr>
        <td><%= issue_history.issue_request %></td>
        <td><%= issue_history.issue_master %></td>
        <td><%= issue_history.description %></td>
        <td><%= issue_history.date %></td>
        <td><%= issue_history.time %></td>
        <td><%= issue_history.employee %></td>
        <td><%= issue_history.issue_tracker_member %></td>
        <td><%= issue_history.issue_tracker_group %></td>
        <td><%= issue_history.issue_priority %></td>
        <td><%= issue_history.status %></td>
        <td><%= issue_history.is_confirm %></td>
        <td><%= link_to 'Show', issue_history %></td>
        <td><%= link_to 'Edit', edit_issue_history_path(issue_history) %></td>
        <td><%= link_to 'Destroy', issue_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue history', new_issue_history_path %>
