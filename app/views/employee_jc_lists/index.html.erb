<p id="notice"><%= notice %></p>

<h1>Listing Employee Jc Lists</h1>

<table>
  <thead>
    <tr>
      <th>Joining checklist master</th>
      <th>Employee</th>
      <th>Admin</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_jc_lists.each do |employee_jc_list| %>
      <tr>
        <td><%= employee_jc_list.joining_checklist_master %></td>
        <td><%= employee_jc_list.employee %></td>
        <td><%= employee_jc_list.admin %></td>
        <td><%= employee_jc_list.status %></td>
        <td><%= link_to 'Show', employee_jc_list %></td>
        <td><%= link_to 'Edit', edit_employee_jc_list_path(employee_jc_list) %></td>
        <td><%= link_to 'Destroy', employee_jc_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

