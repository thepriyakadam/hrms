<p id="notice"><%= notice %></p>

<h1>Listing Nomination Masters</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nomination_masters.each do |nomination_master| %>
      <tr>
        <td><%= nomination_master.code %></td>
        <td><%= nomination_master.name %></td>
        <td><%= nomination_master.description %></td>
        <td><%= link_to 'Show', nomination_master %></td>
        <td><%= link_to 'Edit', edit_nomination_master_path(nomination_master) %></td>
        <td><%= link_to 'Destroy', nomination_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nomination master', new_nomination_master_path %>
