<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee ID</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Employee Name</Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String">Company Name</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">Designation</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Experience</Data></Cell>
        <Cell ss:Index="7"><Data ss:Type="String">CTC</Data></Cell>
        <Cell ss:Index="8"><Data ss:Type="String">Update Time</Data></Cell>
        </Row>

        <% i = 0 %>
        <% @experiences.each do |experience| %>
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i=i+1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= experience.employee.try(:manual_employee_code ) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= experience.employee.try(:first_name ) %>
          <%= experience.employee.try(:middle_name ) %> <%= experience.employee.try(:last_name ) %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= experience.company_name %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= experience.designation %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%= experience.no_of_year %></Data></Cell>
          <Cell ss:Index="7"><Data ss:Type="String"><%= experience.ctc %></Data></Cell>
          <Cell ss:Index="8"><Data ss:Type="String"><%= experience.try(:updated_at).to_date %></Data></Cell>
        </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>