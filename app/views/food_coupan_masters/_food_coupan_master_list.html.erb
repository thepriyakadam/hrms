
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Price</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
   <% @food_coupan_masters.each do |food_coupan_master| %>
      <tr>
        <td><%= food_coupan_master.code %></td>
        <td><%= food_coupan_master.name %></td>
        <td><%= food_coupan_master.description %></td>
        <td><%= food_coupan_master.price %></td>
        <td>
          <%= link_to '', edit_food_coupan_master_path(food_coupan_master),class: 'btn btn-sm btn-success fa fa-edit',remote:true %>
          <%= link_to '',  food_coupan_master, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-sm btn-danger fa fa-close' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>