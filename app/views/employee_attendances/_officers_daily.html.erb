<br>
<div class="box">
  <div class="box-header">
    <div class="box-body">
      <table id="example6" class="table table-bordered table-hover table-condensed">
        <thead>
          <tr>
            <th colspan="7"><center> Date : <%= params[:employee][:from] %></center></th>
          </tr>
          <tr>
            <th>Sr.No.</th>
            <th>Emp. No.</th>
            <th>Employee Name</th>
            <th>In Time</th>
            <th>Remark</th>
            <th>Out Time</th>
            <th>Remark</th>
          </tr>
        </thead>
        <tbody>
          <% i = 0 %>
          <% @employees.each do |e| %>
            <tr>
              <td><%= i = i + 1 %></td>
              <td><%= e.employee.manual_employee_code %></td>
              <td><%= full_name(e.employee) %></td>
              <td><%= e.try(:in_time) %></td>
              <td><%= %></td>
              <td><%= e.try(:out_time) %></td>
              <td></td>
            </tr>
          <% end %> <!-- do -->
        </tbody>
      </table>
      <br>
      <%= link_to " PDF Report ",officers_daily_employee_attendances_path(date: params[:employee][:from], format: "pdf"), class: "btn btn-xl btn-info fa fa-print", target: "_blank" %>
    </div>
  </div> 
</div>