<p id="notice"><%= notice %></p>

<h1>Listing Particular Vacancy Requests</h1>

<table>
  <thead>
    <tr>
      <th>Vacancy master</th>
      <th>Employee</th>
      <th>Open date</th>
      <th>Closed date</th>
      <th>Fulfillment date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @particular_vacancy_requests.each do |particular_vacancy_request| %>
      <tr>
        <td><%= particular_vacancy_request.vacancy_master %></td>
        <td><%= particular_vacancy_request.employee %></td>
        <td><%= particular_vacancy_request.open_date %></td>
        <td><%= particular_vacancy_request.closed_date %></td>
        <td><%= particular_vacancy_request.fulfillment_date %></td>
        <td><%= particular_vacancy_request.status %></td>
        <td><%= link_to 'Show', particular_vacancy_request %></td>
        <td><%= link_to 'Edit', edit_particular_vacancy_request_path(particular_vacancy_request) %></td>
        <td><%= link_to 'Destroy', particular_vacancy_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Particular vacancy request', new_particular_vacancy_request_path %>
