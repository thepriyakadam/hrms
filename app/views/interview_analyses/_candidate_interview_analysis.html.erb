<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Interview evalution</th>
      <th>Interview attribute</th>
      <th>Interview decision</th>
      <th>Comment</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @interview_analyses.each do |interview_analysis| %>
      <tr>
        <td><%= interview_analysis.interview_evalution.name %></td>
        <td><%= interview_analysis.interview_attribute.name %></td>
        <td><%= interview_analysis.interview_decision.name %></td>
        <td><%= interview_analysis.comment %></td>
        <td>
          <%= link_to '', edit_interview_analysis_path(interview_analysis),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
          <%= link_to '',  interview_analysis, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-danger btn-xs fa fa-close' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>