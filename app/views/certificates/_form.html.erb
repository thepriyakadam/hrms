      <div class="form-group required">
        <%= f.text_field :f2_end,label:'End Footer', placeholder: 'Enter The Footer Text' %>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-6">
      <div class="actions">
        <%= f.submit "Create Certificate Format",class: "btn btn-primary btn-sm" %>
       
      </div>
    </div>
  </div>
  <%end%>
</div>
