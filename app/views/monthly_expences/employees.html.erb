<div class="box">
  <div class="box-header">
    <h3 class="box-title">Monthly Expenses</h3>
    <div class="pull-right">
    </div>
    
  </div><!-- /.box-header -->
  <div class="box-body">
  <div id="comments">
    <table id="example1" class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>Employee ID</th>
        <th>Employee Name</th>
        <th>Contact No.</th>
        <th>Action</th>
      </tr>
    </thead>

    <tbody>
       <% @monthly_expences.each do |m| %>
        <tr>
          <td><%= m.employee.try(:manual_employee_code) %></td>
          <td><%= full_name(m.employee) %></td>
          <td><%= m.employee.try(:contact_no) %></td>
          <td>
            <%= link_to '', employee_expences_monthly_expences_path(employee_id: m.employee_id, year: @year, month: @month), class: 'btn btn-xs btn-default fa fa-eye' %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  </div>
  <%= link_to 'Back', monthly_expences_path,class: 'btn btn-default btn-sm fa fa-arrow-left' %>
</div>
</div>