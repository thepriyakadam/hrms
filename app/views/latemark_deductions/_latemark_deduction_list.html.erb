<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Employee</th>
      <th>Latemark day</th>
      <th>Latemark count</th>
      <th>Latemark amount</th>
      <th>Action</th>
    </tr>
    </tr>
  </thead>
  
 <tbody>
    <% @latemark_deductions.each do |latemark_deduction| %>
      <tr>
        <td><%= full_name(latemark_deduction.employee) %></td>
        <td><%= latemark_deduction.latemark_day %></td>
        <td><%= latemark_deduction.latemark_count %></td>
        <td><%= latemark_deduction.latemark_amount %></td>
          <td>
            <%= link_to '',  latemark_deduction, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-danger btn-xs fa fa-close' %>
             <%= link_to '', edit_latemark_deduction_path(latemark_deduction),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
             
          </td>
      </tr>
    <% end %>
  </tbody>
</table>