<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Asset Type</h3>
     <div class="pull-right">
    <%= link_to " PDF ", asset_type_master_asset_types_path(id: @asset_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", asset_type_master_asset_types_path(id: @asset_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
    <div class="box-body">
	    <div id="form">
	      <%= render 'form' %>
	  	</div>
	  	<div id="asset_list">
	    	<%= render 'asset_list' %>
			</div>
   	</div>
</div>
