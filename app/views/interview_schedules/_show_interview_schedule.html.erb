<table id="example" class="table table-bordered table-hover table-condensed">
     <thead>
     <tr>
      <th>Job Title</th>
      <th>Candidate Name</th>
      <th>Interviewer Name</th>
      <th>Interview Date</th>
      <th>Interviewtime</th>
      <th>Location</th>
      </tr>
      </thead>
      <tbody>
  <tr>
        <td><%= @interview_schedule.interviewer_name %></td>
        <td><%= @interview_schedule.candidate_name %></td>
        <td><%= @interview_schedule.employee.try(:manual_employee_code)+' '+@interview_schedule.employee.try(:first_name)+' '+@interview_schedule.employee.try(:last_name) %></td>
        <td><%= @interview_schedule.interview_date %></td>
        <td><%= @interview_schedule.interview_time %></td>
        <td><%= @interview_schedule.location %></td>
  </tr>
  </tbody>
</table>