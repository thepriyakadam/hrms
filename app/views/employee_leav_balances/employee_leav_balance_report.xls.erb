<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee Code</Data></Cell>
				<Cell ss:Index="3"><Data ss:Type="String">Leave Type</Data></Cell>
				<Cell ss:Index="4"><Data ss:Type="String">No Of Leave</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">Total Leave</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">From Date</Data></Cell>
        <Cell ss:Index="7"><Data ss:Type="String">To Date</Data></Cell>
        <Cell ss:Index="8"><Data ss:Type="String">Active</Data></Cell>
        <Cell ss:Index="9"><Data ss:Type="String">Leave Count</Data></Cell>
        <Cell ss:Index="10"><Data ss:Type="String">Collapse Value</Data></Cell>
        <Cell ss:Index="11"><Data ss:Type="String">Carry Forward</Data></Cell>
        <Cell ss:Index="12"><Data ss:Type="String">Working Day</Data></Cell>
				</Row>

        <% i = 0 %>
        <% @employee_leav_balances.each do |employee_leav_balance| %>
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i = i + 1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= employee_leav_balance.employee.try(:manual_employee_code ) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= employee_leav_balance.try(:leav_category).try(:name) %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= employee_leav_balance.try(:no_of_leave) %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= employee_leav_balance.try(:total_leave) %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%= employee_leav_balance.try(:from_date) %></Data></Cell>
          <Cell ss:Index="7"><Data ss:Type="String"><%= employee_leav_balance.try(:to_date) %></Data></Cell>
          <Cell ss:Index="8"><Data ss:Type="String"><%= employee_leav_balance.is_active ? "Active" : "Inactive" %></Data></Cell>
          <Cell ss:Index="9"><Data ss:Type="String"><%= employee_leav_balance.try(:leave_count) %></Data></Cell>
          <Cell ss:Index="10"><Data ss:Type="String"><%= employee_leav_balance.try(:collapse_value) %></Data></Cell>
          <Cell ss:Index="11"><Data ss:Type="String"><%= employee_leav_balance.try(:carry_forward) %></Data></Cell>
          <Cell ss:Index="12"><Data ss:Type="String"><%= employee_leav_balance.try(:working_day) %></Data></Cell>
           </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>