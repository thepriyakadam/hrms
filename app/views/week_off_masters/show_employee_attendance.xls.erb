<table id="example6" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Sr. No</th>
      <th>ID</th>
      <th>Employee</th>
      <th>Date</th>
      <th>Day</th>
      <th>Present</th>
    </tr>
  </thead>
  <tbody>
    <% i = 0 %>
    <% @employee_attendances.each do |e| %>
      <tr>
        <td><%= i = i + 1 %></td>
        <td><%= @employee_attendances.first.employee.manual_employee_code rescue nil %></td>
        <td><%= full_name(@employee_attendances.first.employee) rescue nil %></td>
        <td><%= e.day %></td>
        <td><%= e.employee_week_off.day %></td>
        <td><%= e.present %></td>
      </tr>
    <% end %>
  </tbody>
</table>