<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @nomination_masters.each do |nomination_master| %>
      <tr>
        <td><%= nomination_master.code %></td>
        <td><%= nomination_master.name %></td>
        <td><%= nomination_master.description %></td>
        <td>
          <%= link_to '', edit_nomination_master_path(nomination_master),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>