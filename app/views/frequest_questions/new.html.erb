<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">FAQ's</h3>
  <div class="pull-right">
    <%= link_to " PDF ", frequest_question_master_frequest_questions_path(id: @frequest_question.id, format: "pdf"), class: "btn btn-sm btn-info fa fa-print",:target => "_blank" %>
    <%= link_to " XLS ", frequest_question_master_frequest_questions_path(id: @frequest_question.id, format: "xls"), class: "btn btn-sm btn-success fa fa-print",:target => "_blank" %>
    <%= link_to "Upload FAQ's", import_xl_frequest_questions_path, class: 'btn btn-xs btn-primary' %>
    <%= link_to "Template", "/Templates/20.Frequest Question.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="faq_list">
			<%= render 'faq_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->
