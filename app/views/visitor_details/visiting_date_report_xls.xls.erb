<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Visitor ID </Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Name</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Age </Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String">Contact No.</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">From</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Meet To </Data></Cell>
        <Cell ss:Index="7"><Data ss:Type="String">Entry Time </Data></Cell>
       
      </Row>

       <% @visitor_details.each do |visitor_detail| %>
      <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= visitor_detail.id  %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= visitor_detail.name %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= visitor_detail.age %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= visitor_detail.contact_no %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= visitor_detail.from %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%=full_name(visitor_detail.employee) %></Data></Cell>
          <Cell ss:Index="7"><Data ss:Type="String"><%= visitor_detail.in_time  %></Data></Cell>
          
          </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>
