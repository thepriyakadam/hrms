<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Attribute & Definition</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <% @attribute_masters.each do |attribute_master| %>
      <tr>
        <td><%= attribute_master.name %></td>
        <td>
          <%= link_to '', edit_attribute_master_path(attribute_master),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>