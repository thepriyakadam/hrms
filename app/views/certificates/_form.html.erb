    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :b5_start,label:'Start Body ', placeholder: 'Enter The Body Text' %>
      </div>
    </div> 

    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :b5_end,label:'End Body', placeholder: 'Enter The Body Text' %>
      </div>
    </div>
  </div>

   <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :f1_start,label:'Start Footer', placeholder: 'Enter The Footer Text' %>
