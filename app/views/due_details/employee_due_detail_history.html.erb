<div class="box">
  <div class="box-header">
    <h3 class="box-title">Due Detail History</h3>
<div class="box-body">
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
     <th>No Dues Owner</th>
     <th>No Dues Template</th>
     <th>Employee Name</th>
     <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @due_employee_details.each do |due_employee_detail| %>
      <tr>
       <td><%= due_employee_detail.reporting_master_id %></td>
       <td><%= due_employee_detail.due_template_id %></td>
       <td><%= due_employee_detail.employee.first_name+' '+due_employee_detail.employee.middle_name+' '+due_employee_detail.employee.last_name %></td>
        <td>
          <%= link_to ' Assign No Dues Action', new_due_action_path(due_employee_detail_id: due_employee_detail.id),class: 'btn btn-xs btn-primary' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>