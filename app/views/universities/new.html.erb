<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">University / Institution</h3>
   <div class="pull-right">
    <%= link_to " PDF ", university_master_universities_path(id: @university.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", university_master_universities_path(id: @university.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
    
    <%= link_to "Upload University", import_xl_universities_path, class: 'btn btn-xs btn-primary' %>
    
     <%= link_to "Template", "/Templates/40.University.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="university_li">
			<%= render 'university_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->