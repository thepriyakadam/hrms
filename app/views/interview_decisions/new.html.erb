<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Evaluation</h3>
      <div class="pull-right">
    <%= link_to " PDF ", interview_decision_master_interview_decisions_path(id: @interview_decision.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", interview_decision_master_interview_decisions_path(id: @interview_decision.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
            
    <%= link_to "Upload Evaluation", import_xl_interview_decisions_path, class: 'btn btn-xs btn-primary' %>
    
    <%= link_to "Template", "/Templates/49.Evaluation.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="interview_decision_li">
			<%= render 'interview_decision_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

