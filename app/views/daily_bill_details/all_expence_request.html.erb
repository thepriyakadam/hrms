<div class="box">
  <div class="box-header">
    <h3 class="box-title">Admin Expence Approval</h3>
    <div class="pull-right"></div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed">
      <thead>
        <tr>
          <th>ID</th>
          <th>Employee</th>
          <th>Manager</th>
          <th>place</th>
          <th>Action</th>
        </tr>
      </thead>

      <tbody>
        <% @travel_requests.try(:each) do |fl| %>
          <tr>
            <td><%= fl.employee.try(:manual_employee_code) %></td>
            <td><%= full_name(fl.employee) %></td>
            <td><%= full_name(fl.employee.manager) %></td>
            <td><%= fl.place %></td>
            <td>
             <%= link_to 'Approve', daily_bill_request_confirmation_daily_bill_details_path(fl),class: 'btn btn-sm btn-primary' %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<script type="text/javascript">
$(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]]
    } );
} );
</script>