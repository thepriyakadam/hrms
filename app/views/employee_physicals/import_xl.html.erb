<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Import Physical File</h3>
<div class="box-body">
<%= form_tag import_employee_physicals_path, multipart: true do %>
<label> File </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import", data: { confirm: 'Are you sure?' }, class: 'btn btn-sm btn-primary' %>
<% end %>
</div>
