<table id="example5" class="table table-bordered table-hover table-condensed">
	<thead>
    <tr>
      <th>ID</th>
      <th>Employee Name</th>
      <th>Day</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @employee_attendances.each do |a| %>
      <tr>
        <td><%= a.employee.manual_employee_code %></td>
        <td><%= full_name(a.employee) %></td>
        <td><%= a.day %></td>
        <td><%= a.present %></td>
      </tr>
    <% end %>
  </tbody>
</table>