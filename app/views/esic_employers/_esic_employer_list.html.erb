<div class="box-header with-border">
</div><!-- /.box-header --><!-- 
<div class="box-body"></div> -->
<table id="example6" class="table table-bordered table-hover table-condensed">
 <thead>
    <tr>
      <th>Percentage</th>
      <th>Effective From</th>
      <th>Effective To</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
      <% @esic_employers.each do |esic_employer| %>
      <tr>
        <td><%= esic_employer.percentage %></td>
        <td><%= esic_employer.effective_from %></td>
        <td><%= esic_employer.effective_to %></td>
        <td><%= esic_employer.is_active ? "Active" : "Inactive" %></td>
        <td>
          <% if esic_employer.is_confirm == true %>
            <small class= "label label-primary">Confirmed</small>
            <%= link_to '', esic_employers_modal_esic_employers_path(esic_employer) ,class: 'btn btn-xs btn-default fa fa-eye',remote:true %>
            <%= link_to '', edit_esic_employer_path(esic_employer) ,class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
          <% else %>
            <%= link_to '', edit_esic_employer_path(esic_employer) ,class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
            <%= link_to '',  esic_employer, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-xs btn-danger fa fa-close' %>
            <%= link_to '', is_confirm_esic_employers_path(esic_employer: esic_employer),data: { confirm: 'Are you sure?'},class: 'btn btn-xs btn-info fa fa-check' %>
            <%= link_to '', esic_employers_modal_esic_employers_path(esic_employer) ,class: 'btn btn-xs btn-default fa fa-eye',remote:true %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<div id="Mymodal" class="modal fade"></div>

<script type="text/javascript">
$(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]]
    } );
} );
</script>