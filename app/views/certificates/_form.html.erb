 <div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Create Certificate Format</h3>
  
  <div class="pull-right">

   </div>
  <div class="box-body">
 <%= bootstrap_form_for(@certificate,html: {id: 'certificate'}) do |f| %>
  <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.select :certificate_master_id, all_certificate_master, {include_blank: " Select Certificate"} %>
      </div>
    </div> 
  </div>
 

   <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :h1_start,label:'Start Heading 1 ', placeholder: 'Enter The Heading Text' %>
      </div>
    </div> 
 
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :h1_end,label:'End Heading 1 ', placeholder: 'Enter The Heading Text' %>
      </div>
    </div>
  </div>

   <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
