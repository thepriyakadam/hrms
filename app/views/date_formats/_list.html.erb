<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_formats.each do |date_format| %>
      <tr>
        <td><%= date_format.name %></td>
        <td><%= date_format.value %></td>
         <td>
          <%= link_to '', edit_date_format_path(date_format),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
          <%= link_to '', date_format, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-xs btn-danger fa fa-close',remote: true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>