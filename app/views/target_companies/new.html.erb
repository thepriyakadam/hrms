<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Target Company</h3>
      <div class="pull-right">
    <%= link_to " PDF ", target_company_master_target_companies_path(id: @target_company.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", target_company_master_target_companies_path(id: @target_company.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="target_list">
			<%= render 'target_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->