<%= bootstrap_form_for(@degree_type,html: {id: 'degree_type'},remote:true) do |f| %>
  <div class="row">
    <div class="col-sm-3">
      <div class="form-group required">
        <%= f.text_field :name,hide_label:true, placeholder: 'Degree Type' %>
      </div>
    </div>

    <div class="col-sm-3">
      <div class="actions">
        <%= f.submit :class => class_name, data: { disable_with: data_info },value:'Create Degree Type' %>
      </div>
    </div>
  </div>
<% end %>
