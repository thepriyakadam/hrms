
<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Upload Daily Attendance</h3>
</div><!-- /.box-header -->
<div class="box-body">
<%= form_tag({:action => :upload}, :multipart => true) do %>
<label> File (.xls,.csv,.xlsx only) </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import" %>
<% end %>
