<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee ID</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">First Name</Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String">Middle Name</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">Last Name</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Qualification Level</Data></Cell>
        <Cell ss:Index="7"><Data ss:Type="String">Qualification</Data></Cell>
        <Cell ss:Index="8"><Data ss:Type="String">Qualification Specialization</Data></Cell>
        <Cell ss:Index="9"><Data ss:Type="String">Marks</Data></Cell>
        <Cell ss:Index="10"><Data ss:Type="String">Passout Year</Data></Cell>
        <Cell ss:Index="11"><Data ss:Type="String">College/School</Data></Cell>
        <Cell ss:Index="12"><Data ss:Type="String">University/Institute</Data></Cell>
        </Row>

        <% i = 0 %>
         <% @qualifications.each do |qualification| %>
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i=i+1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= qualification.employee.try(:manual_employee_code ) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= qualification.employee.try(:first_name ) %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= qualification.employee.try(:middle_name ) %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= qualification.employee.try(:last_name ) %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%= qualification.degree_type.try(:name) %></Data></Cell>
          <Cell ss:Index="7"><Data ss:Type="String"><%= qualification.degree.try(:name) %></Data></Cell>
          <Cell ss:Index="8"><Data ss:Type="String"><%= qualification.degree_stream.try(:name) %></Data></Cell>
          <Cell ss:Index="9"><Data ss:Type="String"><%= qualification.marks %></Data></Cell>
          <Cell ss:Index="10"><Data ss:Type="String"><%= qualification.year.try(:name) %></Data></Cell>
          <Cell ss:Index="11"><Data ss:Type="String"><%= qualification.college %></Data></Cell>
          <Cell ss:Index="12"><Data ss:Type="String"><%= qualification.university.try(:name) %></Data></Cell>
           </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>