<p id="notice"><%= notice %></p>

<h1>Listing Employer Pfs</h1>

<table>
  <thead>
    <tr>
      <th>Base component</th>
      <th>Percentage</th>
      <th>Effective from</th>
      <th>Effective to</th>
      <th>Is active</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employer_pfs.each do |employer_pf| %>
      <tr>
        <td><%= employer_pf.base_component %></td>
        <td><%= employer_pf.percentage %></td>
        <td><%= employer_pf.effective_from %></td>
        <td><%= employer_pf.effective_to %></td>
        <td><%= employer_pf.is_active %></td>
        <td><%= employer_pf.is_confirm %></td>
        <td><%= link_to 'Show', employer_pf %></td>
        <td><%= link_to 'Edit', edit_employer_pf_path(employer_pf) %></td>
        <td><%= link_to 'Destroy', employer_pf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

