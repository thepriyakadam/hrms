<p id="notice"><%= notice %></p>

<h1>Listing Employee Week Offs</h1>

<table>
  <thead>
    <tr>
      <th>Week off master</th>
      <th>Employee</th>
      <th>Day</th>
      <th>Date</th>
      <th>Status</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_week_offs.each do |employee_week_off| %>
      <tr>
        <td><%= employee_week_off.week_off_master %></td>
        <td><%= employee_week_off.employee %></td>
        <td><%= employee_week_off.day %></td>
        <td><%= employee_week_off.date %></td>
        <td><%= employee_week_off.status %></td>
        <td><%= employee_week_off.is_confirm %></td>
        <td><%= link_to 'Show', employee_week_off %></td>
        <td><%= link_to 'Edit', edit_employee_week_off_path(employee_week_off) %></td>
        <td><%= link_to 'Destroy', employee_week_off, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee week off', new_employee_week_off_path %>
