
<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Import File</h3>
<div class="pull-right">
	<%= link_to " Template ", "/Templates/62.Employee Attendance Upload.xls", :class=>" fa fa-download btn-xl btn btn-warning" %>
</div>
</div><!-- /.box-header -->

<div class="box-body">
<%= form_tag import_employee_attendances_path, multipart: true do %>
<label> File (.xls,.csv,.xlsx only) </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import" %>
<% end %>
