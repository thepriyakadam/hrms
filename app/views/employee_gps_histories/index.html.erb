<p id="notice"><%= notice %></p>

<h1>Listing Employee Gps Histories</h1>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Location</th>
      <th>From date</th>
      <th>To date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_gps_histories.each do |employee_gps_history| %>
      <tr>
        <td><%= employee_gps_history.member %></td>
        <td><%= employee_gps_history.latitude %></td>
        <td><%= employee_gps_history.longitude %></td>
        <td><%= employee_gps_history.location %></td>
        <td><%= employee_gps_history.from_date %></td>
        <td><%= employee_gps_history.to_date %></td>
        <td><%= link_to 'Show', employee_gps_history %></td>
        <td><%= link_to 'Edit', edit_employee_gps_history_path(employee_gps_history) %></td>
        <td><%= link_to 'Destroy', employee_gps_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

