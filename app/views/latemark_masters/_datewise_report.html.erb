<div class="box">
  <div class="box-header">
    <h3 class="box-title">Latemark Employee List</h3>
  </div><!-- /.box-header -->
  <div class="box-body">

      <table id="example6" class="table table-bordered table-hover table-condensed">
        <thead>
          <tr><th colspan="4"><center>From <%= @from_date.to_date %> To <%= @to_date.to_date %></center></th></tr>
          <tr>
            <th>ID</th>
            <th>Employee</th>
            <th>In Time</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
          <% @emp_att.each do |employee_attendance| %>

            <tr>
                <td><%= employee_attendance.employee.manual_employee_code %></td>
                <td><%= full_name(employee_attendance.employee) %></td>
                <td><%= employee_attendance.in_time.strftime("%I:%M") %></td>
                <td><%= employee_attendance.day %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br>
      <%= link_to " Excel Report ",datewise_report_latemark_masters_path(:latemark_master =>{:from_date => params[:latemark_master][:from_date],:to_date => params[:latemark_master][:to_date]}, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>
      
      <%= link_to " PDF Report ",datewise_report_latemark_masters_path(:latemark_master =>{:from_date => params[:latemark_master][:from_date],:to_date => params[:latemark_master][:to_date]}, format: "pdf"), class: "btn btn-xl btn-info fa fa-print", :target => "_blank" %>

  </div>
</div>

<script type="text/javascript">
$(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]]
    } );
} );
</script>