<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Edit Appraiser Rating</h3>
  </div>
  <div class="box-body">
   <%= bootstrap_form_for(@attribute_rating_sheet) do |f| %>
    <div class= "row">
      <div class="col-sm-3">
      <div class= "field">
      <%= f.text_field :appraiser_comment %>
      </div>
      </div>

      <div class="col-sm-3">
      <div class= "field">
      <%= f.select :appraiser_rating,["1","2","3","4","5"]%>
      </div>
      </div>

      </div>

    <div class="row">
      <div class="col-sm-3">
        <div class="actions">
          <%= f.submit "Update Appraiser Rating" ,class: "btn btn-primary btn-sm"%>
         </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>