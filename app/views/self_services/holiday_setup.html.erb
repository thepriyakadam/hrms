<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Holiday</h3>
</div><!-- /.box-header -->
<div class="box-body">
<table id="example1" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Employee Name</th>
      <th>Day</th>  
      <th>Present</th>
    </tr>
  </thead>
  <tbody>
    <% @employee_attendances.each do |employee_attendance| %>
        <tr>
          <td><%= employee_attendance.employee.try(:manual_employee_code) %></td>
          <td><%= full_name(employee_attendance.employee) %></td>
          <td><%= employee_attendance.day %></td>
          <td><%= employee_attendance.present %></td>
        </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>