<div class="box box-default">
  <div class="box-header with-border">
    <div class="box-tools pull-right">
      <%= link_to 'Back', employees_path,class: 'btn btn-default btn-sm fa fa-arrow-left' %>
      <%= link_to "Add Nominies", ajax_new_employee_nomination_employees_path(id: @employee.id), class: 'btn btn-primary',remote:true %>
    </div>
  </div><!-- /.box-header -->
  <div class="box-body">
   <table id="example2" class="table table-bordered table-hover table-condensed">
    <thead>
     <thead>
    <tr>
      <th>Nomination Master</th>
      <th>Family</th>
      <th>Nomination</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @employee_nominations.each do |employee_nomination| %>
      <tr>
        <td><%= employee_nomination.nomination_master %></td>
        <td><%= employee_nomination.family %></td>
        <td><%= employee_nomination.nomination %></td>
        <td>
        <%= link_to '', employee_nomination,class: 'btn btn-default fa btn-sm fa-eye',remote: true %>
          <!--  link_to '', edit_employee_nomination_path(employee_nomination) ,class: 'btn btn-xs btn-success fa fa-edit',remote:true  %>
           link_to '', employee_nomination, method: :delete, data: { confirm: 'Are you sure?'},class: 'btn btn-xs btn-danger fa fa-close' %> -->
        </td>
      <% end %>
    </tbody>
  </table>
  </div><!-- /.box -->
</div>