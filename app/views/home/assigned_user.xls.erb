<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">ID</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Employee Name</Data></Cell>
    <Cell ss:Index="4"><Data ss:Type="String">System Role</Data></Cell>
    <Cell ss:Index="5"><Data ss:Type="String">Reporting Manager 1</Data></Cell>
     <Cell ss:Index="6"><Data ss:Type="String">Reporting Manager 2</Data></Cell>
    </Row>

        <% i = 0 %>
        <% @employees.each do |employee| %>  
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i=i+1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= employee.try(:manual_employee_code) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= full_name(employee) %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= employee.member.try(:role).try(:name) %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= employee.manager.try(:first_name) %> <%= employee.manager.try(:middle_name) %> <%= employee.manager.try(:last_name) %></Data></Cell>
           <Cell ss:Index="6"><Data ss:Type="String"><%= employee.manager_2.try(:first_name) %> <%= employee.manager_2.try(:middle_name) %><%= employee.manager_2.try(:last_name) %></Data></Cell>
           </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>