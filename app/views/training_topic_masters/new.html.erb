<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Training Course</h3>
     <div class="pull-right">
    <%= link_to " PDF ", training_topic_master_training_topic_masters_path(id: @training_topic_master.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", training_topic_master_training_topic_masters_path(id: @training_topic_master.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to "Upload Training Course", import_xl_training_topic_masters_path, class: 'btn btn-xs btn-primary' %>
    
    <%= link_to "Template", "/Templates/58.Training Course.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="training_topic_master_li">
			<%= render 'training_topic_master_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

