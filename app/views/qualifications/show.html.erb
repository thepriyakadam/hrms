</tr>

<tr>
  <th><strong>Hsc from:</strong></th>
  <td><%= @qualification.hsc_from %></td>

   <th><strong>Hsc marks:</strong></th>
  <td><%= @qualification.hsc_marks %></td>
</tr>

<tr>
  <th><strong>Hsc year:</strong></th>
  <td><%= @qualification.hsc_year %></td>

   <th><strong>Hsc bord:</strong></th>
  <td><%= @qualification.hsc_bord %></td>
</tr>


<tr>
  <th><strong>Ug:</strong></th>
  <td><%= @qualification.ug %></td>

   <th><strong>Ug from:</strong></th>
  <td><%= @qualification.ug_from %></td>
</tr>


<tr>
  <th><strong>Ug marks:</strong></th>
  <td><%= @qualification.ug_marks %></td>

   <th><strong>Ug year:</strong></th>
