<div class="box">
  <div class="box-header">
    <h3 class="box-title">Daily Bill History</h3>
    <div class="pull-right"></div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <div class="row">
      <div class="col-sm-12">
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Expence Date</th>
      <th>E Place</th>
      <th>Travel Expence Type</th>
      <th>Travel Expence</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @daily_bill_details.each do |daily_bill_detail| %>
      <tr>
        <td><%= daily_bill_detail.expence_date %></td>
        <td><%= daily_bill_detail.e_place %></td>
        <td><%= daily_bill_detail.travel_expence_type.name %></td>
        <td><%= daily_bill_detail.travel_expence %></td>       
        <td>
       
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
</div>
