<!-- SELECT2 EXAMPLE -->
<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Employee Qualifications Details</h3>
    <div class="box-tools pull-right">
      <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
      <button class="btn btn-box-tool" data-widget="remove"><i class="fa fa-remove"></i></button>
    </div>
  </div><!-- /.box-header -->

<div class="box-body">
<%= bootstrap_form_for(@company) do |f| %>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
            <%= f.text_field :employee_id %>
          </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
            <%= f.text_field :ssc %>
         </div>
        </div>
      </div>

  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
          <%= f.text_field :ssc_from %>
           </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
            <%= f.text_field :ssc_marks %>
          </div>
        </div>
      </div>

   <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
            <%= f.number_field :ssc_year %>
          </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.text_field :ssc_bord %>
          </div>
        </div>
      </div>

  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.text_field :diploma %>
