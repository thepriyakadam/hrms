<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Name</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Range</Data></Cell>
		<Cell ss:Index="4"><Data ss:Type="String">Last Range</Data></Cell>
		<Cell ss:Index="5"><Data ss:Type="String">Status</Data></Cell>
	  </Row>

        <% i = 0 %>
        <% @employee_code_masters.each do |employee_code_master| %>
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i=i+1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= employee_code_master.name %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= employee_code_master.range %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= employee_code_master.last_range %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= employee_code_master.is_active ? "Active" : "Inactive"%></Data></Cell>
           </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>