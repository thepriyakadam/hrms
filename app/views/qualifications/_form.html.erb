        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field"> 
           <%= f.text_field :diploma_from %>
           </div>
        </div>
      </div>

 <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
            <%= f.text_field :diploma_marks %>
         </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.number_field :diploma_year %>
