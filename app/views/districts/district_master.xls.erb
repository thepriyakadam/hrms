<table id="example1" class="table table-bordered table-hover table-condensed'">
  <thead>
    <tr>  
      <th>Sr. No</th>
      <th>State</th>
      <th>Code</th>
      <th>District</th>
    </tr>
  </thead>
  <tbody>
    <% i = 0 %>
    <% @districts.each do |district| %>
      <tr>
        <td><%= i = i + 1 %></td>
        <td><%= district.state.try(:name) %></td>
        <td><%= district.code %></td>
        <td><%= district.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
