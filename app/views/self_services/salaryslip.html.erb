<div class="box">
  <div class="box-header">
    <h3 class="box-title">Salary Slip</h3>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example1" class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>Salary Slip Code</th>
        <th>Salary Template</th>
        <th>Month</th>
        <th>Year</th>
        <th>Action</th>
      </tr>
  </thead>

  <tbody>
    <% @salary_slips.each do |salary| %>
      <tr>
        <td><%= salary.salary_slip_code %></td>
        <td><%= salary.salary_template.try(:description)%></td>
        <td><%= salary.month %></td>
        <td><%= salary.year %></td>
        <td>
        
         <!-- < link_to '', show_salaryslip_salaryslips_path(salary), class: 'btn btn-default fa fa-eye' %> -->
        < %= link_to '', show_salaryslip_rg_salaryslips_path(salary), class: 'btn btn-default fa fa-eye' %>
        </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>