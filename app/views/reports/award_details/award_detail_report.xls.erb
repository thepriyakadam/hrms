<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Employee Name</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee ID</Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Award name</Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String">Year</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">Award from</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Description</Data></Cell>
      </Row>
         <% i = 0 %>
        <% @awards.each do |award| %>
        <Row>
           <Cell ss:Index="1"><Data ss:Type="String"><%= full_name(award.employee) %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= award.employee.try(:manual_employee_code ) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= award.award_name %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="Number"><%= award.year.try(:name) %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= award.award_from %></Data>
          </Cell>
           <Cell ss:Index="6"><Data ss:Type="String"><%= award.description %></Data></Cell>
          </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>