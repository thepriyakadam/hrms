<table id="example6" class="table table-bordered table-hover table-condensed">
        <thead>
          <tr>
            <th>Sr. No</th>
            <th>ID</th>
            <th>Employee</th>
            <th>Day</th>
            <th>Present</th>
          </tr>
        </thead>
        <tbody>
          <% i = 0 %>
          <% @employee_attendances.each do |e| %>
            <tr>
              <td><%= i = i + 1 %></td>
              <td><%= e.employee.try(:manual_employee_code) %></td>
              <td><%= full_name(e.employee) %></td>
              <td><%= e.day %></td>
              <td><%= e.present %></td>
            </tr>
          <% end %>
        </tbody>
      </table>