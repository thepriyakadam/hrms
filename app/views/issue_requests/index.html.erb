<div class="box">
  <div class="box-header">
    <h3 class="box-title"> Issue Tracker</h3>
    <div class="pull-right">
      <%= link_to "New Issue Request",  new_issue_request_path, class: 'btn btn-primary' %> 
    </div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example1" class="table table-bordered table-hover table-condensed'">
  <thead>
    <tr>
      <th>Issue Master</th>
      <th>Issue Tracker Member</th>
      <th>Issue Tracker Group</th>
      <th>Date</th>
      <th>Time</th>
      <th>Issue Priority</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @issue_requests.each do |issue_request| %>
      <tr>
        <td><%= issue_request.issue_master.try(:name) %></td>
        <td><%= issue_request.issue_tracker_member_id %></td>
        <td><%= issue_request.issue_tracker_group.try(:name) %></td>
        <td><%= issue_request.date %></td>
        <td><%= issue_request.time %></td>
    <!--     <td><= issue_request.employee.try(:first_name) %> <= issue_request.employee.try(:middle_name) %> <= issue_request.employee.try(:last_name) %></td> -->
        <td><%= issue_request.issue_priority %></td>
       <!--  <td><= link_to 'Show', issue_request %></td>
        <td><= link_to 'Edit', edit_issue_request_path(issue_request) %></td>
        <td><= link_to 'Destroy', issue_request, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
        <td><%= link_to '', issue_request, class: 'btn btn-default fa fa-eye btn-sm' %>
            <%= link_to '', edit_issue_request_path(issue_request), class: 'btn btn-sm btn-success fa fa-edit' %>
            <%= link_to '', issue_request, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-sm btn-danger fa fa-close' %>
            </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
<br>