exit
increment = params[:goal_bunch][:r_increment]
@goal_bunch.update(review_comment: comment,reviewer_rating_id: rating,reviewer_id: @employee.id,r_promotion: promotion,r_increment: increment,r_designation: designation,r_ctc: ctc)
designation = params[:goal_bunch][:r_designation]
exit
tp @rep_master = ReportingMaster.where(id: @reporting_master)
@rep_master = ReportingMaster.where(id: @reporting_master)
exit
@employee_ids = params[:employee_ids]
@employee_ids.each do |eid|
@emp_total = @employee_ids.count
exit
@emp_total = @employee_ids.count
