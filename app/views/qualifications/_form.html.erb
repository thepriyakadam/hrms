        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.text_field :hsc %>
          </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.text_field :hsc_from %>
           </div>
        </div>
      </div>

  <div class="row">
        <div class="col-sm-1"></div>
         <div class="col-sm-3">
          <div class="field">
           <%= f.text_field :hsc_marks %>
          </div>
        </div>
      </div>


  <div class="row">
        <div class="col-sm-1"></div>
