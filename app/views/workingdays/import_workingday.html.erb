<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Import Workingday File</h3>
</div><!-- /.box-header -->
<div class="box-body">
<%= form_tag import_workingdays_path, multipart: true do %>
<label> File </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import", data: { confirm: 'Are you sure?' } %>
<% end %>
<p>
Download:
<%= link_to "CSV", import_workingday_workingdays_path(format: "csv") %> |
<%= link_to "Excel", import_workingday_workingdays_path(format: "xls") %>
</p>
