<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Travel Expense Type</h3>
    <div class="pull-right">
    <%= link_to " PDF ", travel_expence_type_master_travel_expence_types_path(id: @travel_expence_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", travel_expence_type_master_travel_expence_types_path(id: @travel_expence_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="travel_expence_type_li">
			<%= render 'travel_expence_type_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

