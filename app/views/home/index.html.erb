<%if current_user.class == Group %>
	<%= render 'group_dashboard' %>
<%elsif current_user.class == Member %>
	<% if current_user.role.try(:name) == 'Company' %>
		<%= render 'company_dashboard' %>
	<% elsif current_user.role.try(:name) == 'CompanyLocation' %>
		<%= render 'company_location_dashboard' %>
	<% elsif current_user.role.try(:name) == 'Department' %>
		<%= render 'department_dashboard' %>
	<% elsif current_user.role.try(:name) == 'Supervisor' %>
		<%= render 'supervisor_dashboard' %>
	<% elsif current_user.role.try(:name) == 'SalaryAccount' %>
		<%= render 'salary_account' %>
	<% elsif current_user.role.try(:name) == 'Account' %>
	<%= render 'account' %>
	<% else %>
		<%= render 'employee_dashboard' %>
	<% end %>
<%end%>