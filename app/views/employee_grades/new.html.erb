<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Employee Grade</h3>
     <div class="pull-right">
    <%= link_to " PDF ", employee_grade_master_employee_grades_path(id: @employee_grade.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", employee_grade_master_employee_grades_path(id: @employee_grade.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
    
    <%= link_to " Upload", import_xl_employee_grades_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template", "/Templates/32.Employee Grade.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="employee_grade_list">
			<%= render 'employee_grade_type_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->
