<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Salary Component</h3>
    <div class="pull-right">
	    <%= link_to " PDF ", salary_component_list_salary_components_path(format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
	    
	    <%= link_to " XLS ", salary_component_list_salary_components_path(format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
	    
	    <%= link_to " Upload ", import_xl_salary_components_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
	    
	     <%= link_to " Template", "/Templates/66.Salary Component.xls", :class=>"fa fa-download  btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		<div class="row">
			<div class="col-sm-12">
				<div id="salary_component_li">
					<%= render 'salary_component_list' %>
				</div>
			</div>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

