<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">About Company</h3>
     <div class="pull-right">
    <%= link_to " PDF ", about_company_master_about_companies_path(id: @about_company.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", about_company_master_about_companies_path(id: @about_company.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to " Upload  ", import_xl_about_companies_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template", "/Templates/55.About Company.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="about_company_li">
			<%= render 'about_company_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

