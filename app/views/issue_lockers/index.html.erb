<p id="notice"><%= notice %></p>

<h1>Listing Issue Lockers</h1>

<table>
  <thead>
    <tr>
      <th>Issue request</th>
      <th>Employee</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_lockers.each do |issue_locker| %>
      <tr>
        <td><%= issue_locker.issue_request %></td>
        <td><%= issue_locker.employee %></td>
        <td><%= issue_locker.status %></td>
        <td><%= link_to 'Show', issue_locker %></td>
        <td><%= link_to 'Edit', edit_issue_locker_path(issue_locker) %></td>
        <td><%= link_to 'Destroy', issue_locker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue locker', new_issue_locker_path %>
