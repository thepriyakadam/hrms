<div class="box">
  <div class="box-header">
    <h3 class="box-title">Role Assigned User</h3>    
  </div><!-- /.box-header -->
  <div class="box-body">
    <table  id="example1" class="table table-bordered table-hover table-condensed">
	    <thead>
	      <tr>
		      <th>Employee ID</th>
		      <th>Employee Name</th>
		      <th>HRMS Access Role</th>
		      <th>Reporting Person One</th>
		      <th>Reporting Person Two</th>
		      <th>Department</th>
		    </tr>
		  </thead>
		  <tbody>
		    <% @employees.each do |employee| %>   
			      <tr>
			        <td><%= employee.manual_employee_code %></td>
			        <td><%= employee.first_name %> <%= employee.middle_name %> <%= employee.last_name %></td>
			        <td><%= employee.member.try(:role).try(:name) %>
        			<td><%= employee.manager.try(:first_name) %> <%= employee.manager.try(:middle_name) %> <%= employee.manager.try(:last_name) %></td>
        			<td><%= employee.manager_2.try(:first_name) %> <%= employee.manager_2.try(:middle_name) %><%= employee.manager_2.try(:last_name) %></td>
			        <td><%= employee.department.try(:name) %></td>
			      </tr>	
		    <% end %>
		  </tbody>
		</table>
	</div>
</div>

<script type="text/javascript">
	$(function () {
		$('#example1').Datatable();
	});
</script>