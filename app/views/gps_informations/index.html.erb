<p id="notice"><%= notice %></p>

<h1>Listing Gps Informations</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Day</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Place</th>
      <th>Radius</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gps_informations.each do |gps_information| %>
      <tr>
        <td><%= gps_information.employee %></td>
        <td><%= gps_information.day %></td>
        <td><%= gps_information.longitude %></td>
        <td><%= gps_information.latitude %></td>
        <td><%= gps_information.place %></td>
        <td><%= gps_information.radius %></td>
        <td><%= link_to 'Show', gps_information %></td>
        <td><%= link_to 'Edit', edit_gps_information_path(gps_information) %></td>
        <td><%= link_to 'Destroy', gps_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gps information', new_gps_information_path %>
