<div class="box">
  <div class="box-header">
    <h3 class="box-title">Solved List</h3>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed'">
      <thead>
        <tr>
          <th>Support</th>
          <th>Date</th>
          <th>Priority</th>
          <th>Action</th>
        </tr>
      </thead>

      <tbody>
        <% @issue_requests.each do |issue_request| %>
          <tr>
            <td><%= issue_request.issue_master.try(:name) %></td>
            <td><%= issue_request.date %></td>
            <td><%= issue_request.issue_priority %></td>
            <td>
              <% if issue_request.is_complete == true %>
                <small class="label label-primary">Complete</small>
              <% else %>
                <%= link_to 'Complete',solved_confirm_issue_requests_path(issue_request),class: 'btn btn-sm btn-info',data: { confirm: 'Are you sure?' } %>
                <%= link_to 'Resend',resend_request_issue_requests_path(issue_request),class: 'btn btn-sm btn-info',data: { confirm: 'Are you sure?' } %>
                <%= link_to '', issue_request,class: 'btn btn-sm btn-default fa fa-eye' %>
              <% end %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<script type="text/javascript">
$(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]]
    } );
} );
</script>