<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Leave Balance - Import File</h3>
<div class="pull-right">
 <%= link_to " Template", "/Templates/12.Employee Leave Balance.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
</div>
<div class="box-body">
<%= form_tag import_employee_leav_balances_path, multipart: true do %>
<label> File </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import" %>
<% end %>
</div>
</div>