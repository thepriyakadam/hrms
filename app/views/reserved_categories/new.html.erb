<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Reservation Category</h3>
    <div class="pull-right">
    <%= link_to " PDF ", reserved_category_master_reserved_categories_path(id: @reserved_category.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", reserved_category_master_reserved_categories_path(id: @reserved_category.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="reserved_category_li">
			<%= render 'reserved_category_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->