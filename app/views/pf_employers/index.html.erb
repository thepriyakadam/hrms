<p id="notice"><%= notice %></p>

<h1>Listing Pf Employers</h1>

<table>
  <thead>
    <tr>
      <th>Base component</th>
      <th>Percentage</th>
      <th>Effective from</th>
      <th>Effective to</th>
      <th>Is active</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pf_employers.each do |pf_employer| %>
      <tr>
        <td><%= pf_employer.base_component %></td>
        <td><%= pf_employer.percentage %></td>
        <td><%= pf_employer.effective_from %></td>
        <td><%= pf_employer.effective_to %></td>
        <td><%= pf_employer.is_active %></td>
        <td><%= pf_employer.is_confirm %></td>
        <td><%= link_to 'Show', pf_employer %></td>
        <td><%= link_to 'Edit', edit_pf_employer_path(pf_employer) %></td>
        <td><%= link_to 'Destroy', pf_employer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pf employer', new_pf_employer_path %>
