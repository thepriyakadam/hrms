<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Company Type</h3>
     <div class="pull-right">
    <%= link_to " PDF ", company_type_master_company_types_path(id: @company_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", company_type_master_company_types_path(id: @company_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="company_list">
			<%= render 'company_type_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->

