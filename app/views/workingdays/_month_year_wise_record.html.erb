<div class="box-header">
	<div class="box-body">
  	<div id="comments">

		  <table id="example5" class="table table-bordered table-hover table-condensed">
		    <thead>
		      <tr>
		        <th>ID</th>
		        <th>Employee Name</th>
		        <th>Month</th>
		        <th>Year</th>
		        <th>Present Day</th>
		      </tr>
		    </thead>

		    <tbody>
		      <% (@d1..@d2).each do |i| %>
		      	<% date = i.to_date %>
      			<% @month = date.strftime('%B') %>
      			<% @year = date.strftime('%Y') %>
      			<% @workingdays = Workingday.where(month_name: @month,year: @year) %>
	      			<% @workingdays.each do |workingday| %>
				        <tr>
				          <td><%= workingday.employee.try(:manual_employee_code) %></td>
				          <td><%= full_name(workingday.employee) %></td>
				          <td><%= workingday.try(:month_name) %></td>
				          <td><%= workingday.try(:year) %></td>
				          <td><%= workingday.present_day %></td>
				        </tr> 
		    		<% end %>
		    	<% end %>
		    </tbody>
		  </table>
 		</div>
 	</div>
 </div>
