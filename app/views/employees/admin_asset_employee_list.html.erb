<div class="box">
  <div class="box-header">
    <h3 class="box-title">Employee List</h3>
    <div class="pull-right">
    </div>
  </div>
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed">
    <thead>
    <tr>
      <th>Asset Type</th>
      <th>Asset Detail</th>
      <th>Asset</th>
      <th>Asset Value</th>
      <th>Asset Status</th>
      <th>Issue Date</th>
      <th>Valid Till</th>
     <!--  <th>Returned on</th> -->
      <th>Remarks</th>
    </tr>
  </thead>

	<tbody>
	<% @assigned_assets.each do |assigned_asset| %>
      <tr>
        <td><%= assigned_asset.asset_type %></td>
        <td><%= assigned_asset.assets_detail %></td>
        <td><%= assigned_asset.assets_id %></td>
        <td><%= assigned_asset.assets_value %></td>
        <td><%= assigned_asset.assest_status %></td>
        <td><%= assigned_asset.issue_date %></td>
        <td><%= assigned_asset.valid_till %></td>
<!--         <td><%= assigned_asset.returned_on %></td>
 -->    <td><%= assigned_asset.remarks %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
  </div>
</div>