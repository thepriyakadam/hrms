<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Contact no</th>
      <th>Skillset</th>
      <th>Degree</th>
      <th>Ctc</th>
      <th>Email</th>
      <th>Experience</th>
      <th>Notice period</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @selected_resumes.each do |selected_resume| %>
      <tr>
        <td><%= selected_resume.name %></td>
        <td><%= selected_resume.contact_no %></td>
        <td><%= selected_resume.skillset %></td>
        <td><%= selected_resume.degree.try(:name) %></td>
        <td><%= selected_resume.ctc %></td>
        <td><%= selected_resume.email_id %></td>
        <td><%= selected_resume.experience %></td>
        <td><%= selected_resume.notice_period %></td>
        <td><%= link_to '', edit_selected_resume_path(selected_resume),class: 'btn btn-success btn-xs fa fa-edit',remote:true %></td>
        <td><%= link_to '', selected_resume, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger btn-xs fa fa-close' %></td>
      </tr>
    <% end %>
  </tbody>
</table>