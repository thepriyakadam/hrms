<div class="box">
  <div class="box-header">
    <h3 class="box-title">Subordinate List</h3>
    <div class="pull-right">  
    </div>
  </div>
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover table-condensed">
      <thead>
      	<tr>
      		<th>Employee Id</th>
      		<th>Employee</th>
      		<th>Department</th>
      		<th>Action</th>
      	</tr>
 			</thead>
 			<tbody>
        <% @employees.each do |e| %>
        <tr>
        	<td><%= e.manual_employee_code %></td>
          <td><%= e.first_name.to_s+" "+e.last_name.to_s %></td>
          <td><%= e.department.try(:name) %></td>
          <td><%= link_to '', employee_details_attribute_rating_sheets_path(e) ,class: 'btn btn-xs btn-default fa fa-eye' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
