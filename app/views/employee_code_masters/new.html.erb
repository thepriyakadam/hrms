<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Employee Code Master</h3>

      <div class="pull-right">
    <%= link_to " PDF ", employee_code_master_employee_code_masters_path(id: @employee_code_master.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", employee_code_master_employee_code_masters_path(id: @employee_code_master.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to " Upload ", import_xl_employee_code_masters_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template", "/Templates/25.Employee Code Master.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="employee_code_masters_li">
			<%= render 'employee_code_masters_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->
