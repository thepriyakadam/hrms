<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Policy Type</h3>
     <div class="pull-right">
    <%= link_to " PDF ", policy_type_master_policy_types_path(id: @policy_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", policy_type_master_policy_types_path(id: @policy_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="policy_type_list">
			<%= render 'policy_type_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->