<br>
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr> 
      <th>Compliance type</th>
      <th>Type</th>
      <th>Submission date</th>
      <th>Reminder date</th>
      <th>Previous days</th>
      <th>After days</th>
      <th>Email 1</th>
      <th>Email 2</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <% @transaction_records.each do |transaction_record| %>
      <tr>
        <td><%= transaction_record.try(:compliance_type).try(:name) %></td>
        <td><%= transaction_record.transaction_type %></td>
        <td><%= transaction_record.submission_date %></td>
        <td><%= transaction_record.reminder_Date %></td>
        <td><%= transaction_record.previous_days %></td>
        <td><%= transaction_record.after_days %></td>
        <td><%= transaction_record.email_1 %></td>
        <td><%= transaction_record.email_2 %></td>
        <td><%= transaction_record.status %></td>
        <td>
            <%= link_to '', edit_transaction_record_path(transaction_record),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
            <%= link_to '', transaction_record, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-xs btn-danger fa fa-close' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
