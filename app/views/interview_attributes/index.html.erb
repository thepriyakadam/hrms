<p id="notice"><%= notice %></p>

<h1>Listing Interview Attributes</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interview_attributes.each do |interview_attribute| %>
      <tr>
        <td><%= interview_attribute.code %></td>
        <td><%= interview_attribute.name %></td>
        <td><%= interview_attribute.description %></td>
        <td><%= link_to 'Show', interview_attribute %></td>
        <td><%= link_to 'Edit', edit_interview_attribute_path(interview_attribute) %></td>
        <td><%= link_to 'Destroy', interview_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interview attribute', new_interview_attribute_path %>
