<div class="box">
  <div class="box-header">
    <h3 class="box-title">Selected Employee Transfer List</h3>
    
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example1" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Employee</th>
      <th>Reporting Master</th>
      <th>Employee Designation</th>
      <th>Employee Category</th>
      <th>Company</th>
      <th>Company Location</th>
      <th>Department</th>
      <th>Justification</th>
    </tr>
  </thead>

  <tbody>
    <% @employee_transfers.each do |employee_transfer| %>
      <tr>
        <td><%= employee_transfer.employee.try(:manual_employee_code) %></td>
        <td><%= employee_transfer.reporting_master_id%></td>
        <td><%= employee_transfer.designation %></td>
        <td><%= employee_transfer.category %></td>
        <td><%= employee_transfer.employee_company %></td>
        <td><%= employee_transfer.employee_company_location %></td>  
        <td><%= employee_transfer.employee_department %></td>
        <td><%= employee_transfer.justification %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>
</div>
 <%= link_to " Excel Report ",print_transfer_employee_name_report_employee_transfers_path(employee_id: params[:employee_transfer][:employee_id], format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>
 
<%= link_to " Print Report ",print_transfer_employee_name_report_employee_transfers_path(employee_id: params[:employee_transfer][:employee_id],format: "pdf"), class: "btn btn-xl btn-info fa fa-print", :target => "_blank" %>
</div>