<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Blood Group</h3>
    <div class="pull-right">
    <%= link_to " PDF ", blood_group_master_blood_groups_path(id: @blood_group.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", blood_group_master_blood_groups_path(id: @blood_group.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to "Upload Blood Group", import_xl_blood_groups_path, class: 'btn btn-xs btn-primary' %>
    
    <%= link_to "Template", "/Templates/30.Blood.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="blood_group_list">
			<%= render 'blood_group_type_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->