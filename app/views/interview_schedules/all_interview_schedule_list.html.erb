<div class="box">
  <div class="box-header">
    <h3 class="box-title">Interview Schedule Report</h3>
<div class="box-body">
<table id="example" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Sr.No</th>
      <th>Candidate Name</th>
      <th>Location</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% i = 0 %>
    <% @interview_schedules.each do |interview_schedule| %>
      <tr>
        <td><%= i=i+1 %></td>
        <td><%= interview_schedule.candidate_name %></td>
        <td><%= interview_schedule.location %></td>
        <td><%= link_to ' Report',final_report_interview_schedules_path(interview_schedule),class: 'btn btn-xs btn-primary' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>
</div>
<script>
$(document).ready(function() {
    $('#example').DataTable();
} );
</script>