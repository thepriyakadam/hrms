<div class="box">
  <div class="box-header">
    <h3 class="box-title">My Team For Appraiser</h3>
    <div class="pull-right">  
    </div>
  </div>
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover table-condensed">
     <thead>
        <tr>
          <th>Employee ID</th>
          <th>Name</th>
          <th>Department</th>
          <th>Designation</th>
          <th>Contact No.</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% @employees.each do |e| %>
        <tr>
        <td><%= e.manual_employee_code %></td>
        <td><%= e.first_name %> <%= e.middle_name %> <%= e.last_name %> </td>
        <td><%= e.company_location.try(:name) %> - <%= e.department.try(:name) %></td> 
        <td><%= e.joining_detail.employee_designation.try(:name) %></td>
        <td><%= e.contact_no %></td>
        <td><%= link_to 'Show Goal List', period_list_appraiser_goal_bunches_path(id: e) ,class: 'btn btn-xs btn-info' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>