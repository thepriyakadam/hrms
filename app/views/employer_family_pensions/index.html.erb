<p id="notice"><%= notice %></p>

<h1>Listing Employer Family Pensions</h1>

<table>
  <thead>
    <tr>
      <th>Base component</th>
      <th>Percentage</th>
      <th>Effective from</th>
      <th>Effective to</th>
      <th>Is active</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employer_family_pensions.each do |employer_family_pension| %>
      <tr>
        <td><%= employer_family_pension.base_component %></td>
        <td><%= employer_family_pension.percentage %></td>
        <td><%= employer_family_pension.effective_from %></td>
        <td><%= employer_family_pension.effective_to %></td>
        <td><%= employer_family_pension.is_active %></td>
        <td><%= employer_family_pension.is_confirm %></td>
        <td><%= link_to 'Show', employer_family_pension %></td>
        <td><%= link_to 'Edit', edit_employer_family_pension_path(employer_family_pension) %></td>
        <td><%= link_to 'Destroy', employer_family_pension, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employer family pension', new_employer_family_pension_path %>
