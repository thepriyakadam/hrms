    <tr>
      <th>Name</th>
      <th>Contact no</th>
      <th>Degree</th>
      <th>Ctc</th>
      <th>Experience</th>
      <th>Notice period</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @selected_resume.name %></td>
        <td><%= @selected_resume.contact_no %></td>
