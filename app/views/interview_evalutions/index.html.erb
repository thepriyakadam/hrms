<p id="notice"><%= notice %></p>

<h1>Listing Interview Evalutions</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interview_evalutions.each do |interview_evalution| %>
      <tr>
        <td><%= interview_evalution.code %></td>
        <td><%= interview_evalution.name %></td>
        <td><%= interview_evalution.description %></td>
        <td><%= link_to 'Show', interview_evalution %></td>
        <td><%= link_to 'Edit', edit_interview_evalution_path(interview_evalution) %></td>
        <td><%= link_to 'Destroy', interview_evalution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interview evalution', new_interview_evalution_path %>
