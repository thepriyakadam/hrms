<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Nationality</h3>
    <div class="pull-right">
    <%= link_to " PDF ", nationality_master_nationalities_path(id: @nationality.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", nationality_master_nationalities_path(id: @nationality.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to "Upload Nationality", import_xl_nationalities_path, class: 'btn btn-xs btn-primary' %>
    
    <%= link_to "Template", "/Templates/27.Nationality.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="nationality_list">
			<%= render 'nationality_type_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->