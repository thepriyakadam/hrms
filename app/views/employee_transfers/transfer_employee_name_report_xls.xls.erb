<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">ID </Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee Designation </Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String">Employee Category</Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String">Company</Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String">Company Location </Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Department </Data></Cell>

          <% @employee_transfers.each do |employee_transfer| %>
      <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= employee_transfer.employee.try(:manual_employee_code) %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= employee_transfer.designation %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= employee_transfer.category %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= employee_transfer.employee_company %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= employee_transfer.employee_company_location %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%= employee_transfer.employee_department %></Data></Cell>
          </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>
