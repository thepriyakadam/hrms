<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Date</th>
      <th>Precision</th>
      <th>Timeformat</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting_masters.each do |setting_master| %>
      <tr>
        <td><%= setting_master.date %></td>
        <td><%= setting_master.precision %></td>
        <td><%= setting_master.timeformat %></td>
        <td><%= setting_master.email %></td>
        <td>
          <%= link_to '', edit_setting_master_path(setting_master),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
          <%= link_to '', setting_master, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-xs btn-danger fa fa-close',remote: true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting master', new_setting_master_path %>