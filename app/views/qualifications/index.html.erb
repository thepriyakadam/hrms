<div class="box">
  <div class="box-header">
    <h3 class="box-title">Listing Qualifications</h3>
    <div class="pull-right">
      <%= link_to "New Qualification", new_qualification_path, class: 'btn btn-primary' %> 
    </div>
    
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover">
    <thead>
      <tr>
        <th>SSC</th>
        <th>SSC MARKS</th>
        <th>HSC</th>
        <th>HSC MARKS</th>
        <th>UG</th>
        <th>UG MARKS</th>
        <th>PG</th>
        <th>PG MARKS</th>
        <th>Action</th>
      </tr>
    </thead>

    <tbody>
       <% @qualifications.each do |qualification| %>
        <tr>
          <td><%= qualification.ssc %></td>
          <td><%= qualification.ssc_marks %></td>
          <td><%= qualification.hsc %></td>
          <td><%= qualification.hsc_marks %></td>
          <td><%= qualification.ug %></td>
          <td><%= qualification.ug_marks %></td>
          <td><%= qualification.pg %></td>
          <td><%= qualification.pg_marks %></td>
          <td>
            <%= link_to '', qualification,class: 'btn btn-default fa fa-eye' %>
            <%= link_to '', edit_qualification_path(qualification),class: 'btn btn-success fa fa-edit' %>
            <%= link_to '', qualification, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger fa fa-close' %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>
