<div class="box">
  <div class="box-header">
    <h3 class="box-title">Member</h3>
     <div class="pull-right">
      <%= link_to 'New Member', new_membership_path, class: 'btn btn-sm btn-primary' %> 
    </div>
    </div>

<div class="box-body">
<table id="example6" class="table table-bordered table-hover table-condensed'">
  <thead>
    <tr>
      <th>ID</th>
      <th>Member</th>
      <th>Member Type</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @memberships.each do |membership| %>
      <tr>
        <td><%= membership.employee.try(:manual_employee_code) %></td>
        <td><%= full_name(membership.employee) %></td>
        <td><%= membership.membership_type.try(:name) %></td>
        <td><%= membership.status ? "Active" : "Inactive" %></td>
        <td><%= link_to '', membership,class: 'btn btn-xs btn-default fa fa-eye' %>
           <!-- <= link_to '', edit_membership_path(membership), class: 'btn btn-xs btn-success fa fa-edit' %>
           <= link_to '', membership, method: :delete, data: { confirm: 'Are you sure?' } , class: 'btn btn-xs btn-danger fa fa-trash'%> --></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>

<script type="text/javascript">
$(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]],
        "scrollX": true
    } );
} );
</script>