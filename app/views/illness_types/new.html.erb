<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Illness Type</h3>
    <div class="pull-right">
    <%= link_to " PDF ", illness_type_master_illness_types_path(id: @illness_type.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", illness_type_master_illness_types_path(id: @illness_type.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to "Upload Illness Type", import_xl_illness_types_path, class: 'btn btn-xs btn-primary' %>
    
    <%= link_to "Template", "/Templates/31.Illness.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="illness_type_li">
			<%= render 'illness_type_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

