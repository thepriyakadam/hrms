<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Goal Measure</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <% @goal_measures.each do |goal_measure| %>
      <tr>
        <td><%= goal_measure.name %></td>
        <td>
          <%= link_to '', edit_goal_measure_path(goal_measure),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>