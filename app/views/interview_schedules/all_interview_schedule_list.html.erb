<div class="box">
  <div class="box-header">
    <h3 class="box-title">Report</h3>
<div class="box-body">
<table id="example" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Job Title</th>
      <th>Candidate Name</th>
      <th>Interviewer Name</th>
      <th>Interview Date</th>
      <th>Interview Time</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @interview_schedules.each do |interview_schedule| %>
      <tr>
        <td><%= interview_schedule.interviewer_name %></td>
        <td><%= interview_schedule.candidate_name %></td>
        <td><%= interview_schedule.employee.first_name+' '+interview_schedule.employee.middle_name+' '+interview_schedule.employee.last_name %></td>
        <td><%= interview_schedule.interview_date %></td>
        <td><%= interview_schedule.interview_time %></td>
        <td><%= link_to ' Report',final_report_interview_schedules_path(interview_schedule),class: 'btn btn-xs btn-primary' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>
</div>
<script>
$(document).ready(function() {
    $('#example').DataTable();
} );
</script>