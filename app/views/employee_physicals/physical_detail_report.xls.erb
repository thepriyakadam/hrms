<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:Index="1"><Data ss:Type="String">Sr.No.</Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String">Employee Code</Data></Cell>
				<Cell ss:Index="3"><Data ss:Type="String">Hieght</Data></Cell>
				<Cell ss:Index="4"><Data ss:Type="String">Weight</Data></Cell>
				<Cell ss:Index="5"><Data ss:Type="String">T-Shirt Size</Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String">Trouser Size</Data></Cell>
        </Row>
         <% i = 0 %>
         <% @employee_physicals.each do |employee_physical| %>
        <Row>
          <Cell ss:Index="1"><Data ss:Type="String"><%= i = i + 1 %></Data></Cell>
          <Cell ss:Index="2"><Data ss:Type="String"><%= employee_physical.employee.try(:manual_employee_code ) %></Data></Cell>
          <Cell ss:Index="3"><Data ss:Type="String"><%= employee_physical.height %></Data></Cell>
          <Cell ss:Index="4"><Data ss:Type="String"><%= employee_physical.weight %></Data></Cell>
          <Cell ss:Index="5"><Data ss:Type="String"><%= employee_physical.size %></Data></Cell>
          <Cell ss:Index="6"><Data ss:Type="String"><%= employee_physical.trouser_size %></Data></Cell>
          </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>