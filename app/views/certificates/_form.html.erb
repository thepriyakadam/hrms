      <div class="form-group required">
        <%= f.text_field :h2_end,label:'End Heading 2 ', placeholder: 'Enter The Heading Text' %>
      </div>
    </div>
  </div>


   <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :b1_start,label:'Start Body ', placeholder: 'Enter The Body Text' %>
      </div>
    </div> 
 
    <div class="col-sm-6">
      <div class="form-group required">
        <%= f.text_field :b1_end,label:'End Body', placeholder: 'Enter The Body Text' %>
      </div>
    </div>
  </div>


   <div class="row">
    <div class="col-sm-6">
      <div class="form-group required">
