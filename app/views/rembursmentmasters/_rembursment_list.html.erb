<br>
<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Action</th>
 
  <tbody>
    <% @rembursmentmasters.each do |rembursmentmaster| %>
      <tr>
        <td><%= rembursmentmaster.code %></td>
        <td><%= rembursmentmaster.name %></td>
        <td><%= rembursmentmaster.description %></td>
         <td><%= rembursmentmaster.status ? "Active" : "Inactive" %></td>
        <td>
            <%= link_to '', edit_rembursmentmaster_path(rembursmentmaster),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
            <%= link_to '',  rembursmentmaster, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger btn-xs fa fa-close',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>