<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
      <Cell ss:Index="1"><Data ss:Type="String">Employee ID</Data></Cell>
      <Cell ss:Index="2"><Data ss:Type="String">Employee Name</Data></Cell>
      <Cell ss:Index="3"><Data ss:Type="String">Bank Name</Data></Cell>
      <Cell ss:Index="4"><Data ss:Type="String">Account No.</Data></Cell>
      <Cell ss:Index="5"><Data ss:Type="String">Date</Data></Cell>
      <Cell ss:Index="6"><Data ss:Type="String">Hours</Data></Cell>
     <!--  <Cell ss:Index="7"><Data ss:Type="String">Wages</Data></Cell> -->
      <Cell ss:Index="7"><Data ss:Type="String">OT Amount</Data></Cell>
      <Cell ss:Index="8"><Data ss:Type="String">ESIC</Data></Cell>
      <Cell ss:Index="9"><Data ss:Type="String">OT Paid</Data></Cell>
      <Cell ss:Index="10"><Data ss:Type="String">Att Bonus</Data></Cell>
      <Cell ss:Index="11"><Data ss:Type="String">PH Amount</Data></Cell>
      <Cell ss:Index="12"><Data ss:Type="String">Net Amount</Data></Cell>
  </Row>
      <% i = 0 %>
      <% @overtime_salaries.each do |overtime_salary| %>
      <Row>
      	<Cell ss:Index="1"><Data ss:Type="String"><%= overtime_salary.employee.try(:manual_employee_code) %></Data></Cell>
        <Cell ss:Index="2"><Data ss:Type="String"><%= overtime_salary.employee.try(:first_name)%> <%= overtime_salary.employee.try(:last_name)%></Data></Cell>
        <Cell ss:Index="3"><Data ss:Type="String"><%= overtime_salary.employee.employee_bank_detail.try(:bank_name) %></Data></Cell>
        <Cell ss:Index="4"><Data ss:Type="String"><%= overtime_salary.employee.employee_bank_detail.try(:account_no) %></Data></Cell>
        <Cell ss:Index="5"><Data ss:Type="String"><%= overtime_salary.ot_date %></Data></Cell>
        <Cell ss:Index="6"><Data ss:Type="String"><%= overtime_salary.ot_hrs %></Data></Cell>
       <!--  <Cell ss:Index="7"><Data ss:Type="String"><%= overtime_salary.basic_amount %></Data></Cell> -->
        <Cell ss:Index="7"><Data ss:Type="String"><%= overtime_salary.ot_amount %></Data></Cell>
        <Cell ss:Index="8"><Data ss:Type="String"><%= overtime_salary.ot_esic_amount %></Data></Cell>
        <Cell ss:Index="9"><Data ss:Type="String"><%= overtime_salary.total_amount %></Data></Cell>
        <Cell ss:Index="10"><Data ss:Type="String"><%= overtime_salary.attendence_bouns_amount %></Data></Cell>
        <Cell ss:Index="11"><Data ss:Type="String"><%= overtime_salary.paid_holiday_amount %></Data></Cell>
        <Cell ss:Index="12"><Data ss:Type="String"><%= '%.2f'%(overtime_salary.net_payble_amount) %></Data></Cell>
         </Row><!-- Row -->
      <% end %><!-- slip -->
    </Table>
  </Worksheet>
</Workbook>