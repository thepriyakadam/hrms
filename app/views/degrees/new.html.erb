<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Qualification</h3>
    <div class="pull-right">
    <%= link_to " PDF ", print_qualification_degrees_path(id: @degree.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", print_qualification_degrees_path(id: @degree.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>
    
    <%= link_to "Upload Qualification", import_xl_degrees_path, class: 'btn btn-xs btn-primary' %>
    
     <%= link_to "Template", "/Templates/37.Qualification.xls", :class=>" btn-xs btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="degree_list">
			<%= render 'degree_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->