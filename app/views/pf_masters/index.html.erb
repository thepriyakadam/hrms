<div class="box">
  <div class="box-header">
    <h3 class="box-title">PF Master List</h3>
    <div class="pull-right">
      <%= link_to "Create PF Master", new_pf_master_path, class: 'btn btn-sm btn-primary' %> 
    </div>
    
  </div><!-- /.box-header -->
  <div class="box-body">
  <div id="comments">
    <table id="example2" class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
      <th>Is pf</th>
      <th>Percentage</th>
      <th>Date effective</th>
      <th>Min limit</th>
      <th>Base component</th>
      <th>Is active</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @pf_masters.each do |pf_master| %>
      <tr>
        <td><%= pf_master.is_pf %></td>
        <td><%= pf_master.percentage %></td>
        <td><%= pf_master.date_effective %></td>
        <td><%= pf_master.min_limit %></td>
        <td><%= pf_master.base_component %></td>
        <td><%= pf_master.is_active %></td>
        <td>
        <%= link_to '', pf_master,class: 'btn btn-default fa fa-eye' %>
            <%= link_to '', edit_pf_master_path(pf_master),class: 'btn btn-sm btn-success fa fa-edit' %>
            <%= link_to '', pf_master, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger btn-sm fa fa-close' %>
        </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  </div>
</div>
</div>