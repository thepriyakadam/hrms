<p id="notice"><%= notice %></p>

<h1>Listing Attribute Masters</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Definition</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attribute_masters.each do |attribute_master| %>
      <tr>
        <td><%= attribute_master.code %></td>
        <td><%= attribute_master.name %></td>
        <td><%= attribute_master.definition %></td>
        <td><%= attribute_master.status %></td>
        <td><%= link_to 'Show', attribute_master %></td>
        <td><%= link_to 'Edit', edit_attribute_master_path(attribute_master) %></td>
        <td><%= link_to 'Destroy', attribute_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attribute master', new_attribute_master_path %>
