
<div class="box box-default">
<div class="box-header with-border">
<h3 class="box-title">Workingday - Import Working Day File</h3>
<div class="pull-right">
 <%= link_to " Template", "/Templates/13.Workingday Detail.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
</div>
<div class="box-body">
<%= form_tag import_day_workingdays_path, multipart: true do %>
<label> File (.xls,.csv,.xlsx only) </label>
  <%= file_field_tag :file,label: 'File(.xls,.csv,.xlsx only)' %>
  <br>
  <%= submit_tag "Import", data: { confirm: 'Are you sure?' } %>
<% end %>
</div>
