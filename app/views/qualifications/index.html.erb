<div class="box">
  <div class="box-header">
    <h3 class="box-title">Listing Qualifications</h3>
    <div class="pull-right">
      <%= link_to "New Qualification", new_qualification_path, class: 'btn btn-sm btn-primary' %> 
    </div>
    
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>Qualification</th>
        <th>Marks</th>
        <th>Passout Year</th>
        <th>College/School</th>
        <th>University/Institute</th>
        <th>Action</th>
      </tr>
    </thead>

    <tbody>
       <% @qualifications.each do |qualification| %>
        <tr>
          <td><%= qualification.course %></td>
          <td><%= qualification.marks %></td>
          <td><%= qualification.year_id %></td>
          <td><%= qualification.college %></td>
          <td><%= qualification.university %></td>
         <td>
            <%= link_to '', qualification,class: 'btn btn-default btn-sm fa fa-eye' %>
            <%= link_to '', edit_qualification_path(qualification),class: 'btn btn-success btn-sm fa fa-edit' %>
            <%= link_to '', qualification, method: :delete, data: { confirm: 'Are you sure?' },class: 'btn btn-danger btn-sm fa fa-close' %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
</div>

<script type="text/javascript">
  $(document).ready(function() {
    $('#example6').DataTable( {
        "lengthMenu": [[10, 25, 50,100, -1], [10, 25, 50,100, "All"]]
    } );
} );
</script>