<div class="box">
  <div class="box-header">
    <h3 class="box-title">Employee List</h3>
    <div class="pull-right">
    </div>
  </div>
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed">
    <thead>
   <tr>
      <th>Sr.No.</th>
      <th>Name</th>
      <th>Skill Level</th>
    <!--   <th>Action</th> -->
    </tr>
    </thead>
  <% i = 0%>
    <% @skillsets.each do |skillset| %>
    <tbody>
      <tr>
        <td><%= i = i+1 %></td>
        <td><%= skillset.name %></td>
        <td><%= skillset.skill_level %></td>
        <!-- <td><= link_to ' ', update_skillset_modal_employees_path(skillset),class: 'btn btn-success btn-sm fa fa-edit',remote: true %></td> -->

      </tr>
        <%end%>
        </tbody>
    </table><br>
    <%= link_to 'Back',new_employee_list_employees_path,class: 'btn btn-default btn-sm fa fa-arrow-left'%>
  </div><!-- /.box -->
</div>

<div id="update_skillset_modal" class="modal fade"></div>

