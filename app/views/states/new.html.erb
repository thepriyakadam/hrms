<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">State</h3>
    <div class="pull-right">
	    <%= link_to " PDF ", state_master_states_path(id: @state.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
	    
	    <%= link_to " XLS ", state_master_states_path(id: @state.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>
	    
	    <%= link_to " Upload ", import_xl_states_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
	    
	     <%= link_to " Template", "/Templates/71.State.xls", :class=>"fa fa-download  btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="state_list">
			<%= render 'state_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->