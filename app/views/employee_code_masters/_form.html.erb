<%= bootstrap_form_for(@employee_code_master,html: {id: 'employee_code_master'},remote:true) do |f| %>
  <div class="row">
   <div class="col-sm-3">
      <div class="form-group required">
        <%= f.text_field :name,label: "Name" %>
      </div>
    </div>

    <div class="col-sm-3">
      <div class="form-group required">
        <%= f.text_field :range,label: "Range" %>
      </div>
    </div>

    <div class="col-sm-3">
      <div class="form-group required">
        <%= f.check_box :is_active,label: "Active" %>
      </div>
    </div>
    </div>
    <div class="row">
    <div class="col-sm-3">
      <div class="actions">
        <%= f.submit "Create",:class => class_name, data: { disable_with: data_info } %>
      </div>
    </div>
  </div>
<% end %><br>
