<div class="box">
  <div class="box-header">
    <h3 class="box-title">Approval Process</h3>
    <div class="pull-right">
    </div>
    </div>

<div class="box-body">

<table id="example" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Member</th>
      <th>From</th>
      <th>To</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @approval_processes.each do |approval_process| %>
      <tr>
        <td><%= full_name(approval_process.membership.employee) %></td>
        <td><%= approval_process.from %></td>
        <td><%= approval_process.to %></td>
        <td><%= approval_process.status ? "Active" : "Inactive" %></td>
        <td><%= link_to '', approval_process,class: 'btn btn-xs btn-default fa fa-eye' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>