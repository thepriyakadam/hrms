<p id="notice"><%= notice %></p>

<h1>Listing Employer Esics</h1>

<table>
  <thead>
    <tr>
      <th>Base component</th>
      <th>Percentage</th>
      <th>Effective from</th>
      <th>Effective to</th>
      <th>Is active</th>
      <th>Is confirm</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employer_esics.each do |employer_esic| %>
      <tr>
        <td><%= employer_esic.base_component %></td>
        <td><%= employer_esic.percentage %></td>
        <td><%= employer_esic.effective_from %></td>
        <td><%= employer_esic.effective_to %></td>
        <td><%= employer_esic.is_active %></td>
        <td><%= employer_esic.is_confirm %></td>
        <td><%= link_to 'Show', employer_esic %></td>
        <td><%= link_to 'Edit', edit_employer_esic_path(employer_esic) %></td>
        <td><%= link_to 'Destroy', employer_esic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

