
<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Expence Master</h3>
      <div class="pull-right">
    <%= link_to " PDF ", expence_detail_expenses_masters_path(id: @expenses_master.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", expence_detail_expenses_masters_path(id: @expenses_master.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>

    <%= link_to " Upload ", import_xl_expenses_masters_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template ", "/Templates/69.ExpenceMaster.xls", class: "fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <div id="form">
      <%= render 'form' %>  
    </div>
    
    <div id="expence_master_list">
      <%= render 'expence_master_list' %>
    </div>
    
  </div><!-- /.box-body -->
</div><!-- /.box -->
