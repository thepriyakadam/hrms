<div class="box">
  <div class="box-header">
    <h3 class="box-title">Employee List</h3>
    <div class="pull-right">  
    </div>
  </div>
  <div class="box-body">
    <table id="example1" class="table table-bordered table-hover table-condensed">
       <thead>
        <tr>
          <th>Employee ID</th>
          <th>Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% @employees.each do |e| %>
        <tr>
        <td><%= e.manual_employee_code %></td>
        <td><%= e.first_name %> <%= e.middle_name %> <%= e.last_name %> </td>
          <td><%= link_to 'Details', employee_final_details_attribute_rating_sheets_path(e) ,class: 'btn btn-xs btn-info' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
