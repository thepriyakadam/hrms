<div class="box">
  <div class="box-header">
    <h3 class="box-title">Employee List</h3>
    <div class="pull-right">  
    </div>
  </div>
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover table-condensed">
     <thead>
        <tr>
          <th>Employee ID</th>
          <th>Employee Name</th>
          <th>Department</th>
          <th>Designation</th>
          <th>Rating</th>
        </tr>
      </thead>
      <tbody>
        <% @goal_bunches.each do |e| %>
        <tr>
        <td><%= e.employee.manual_employee_code %></td>
        <td><%= e.employee.first_name %> <%= e.employee.middle_name %> <%= e.employee.last_name %> </td>
        <td><%= e.employee.try(:company_location).try(:name) %> - <%= e.employee.try(:department).try(:name) %></td> 
        <td><%= e.employee.try(:joining_detail).try(:employee_designation).try(:name) %></td>
        <td><%= e.final_rating_id %></td>
        </tr>
        <% end %>
      </tbody>
    </table>   
  </div>
</div>