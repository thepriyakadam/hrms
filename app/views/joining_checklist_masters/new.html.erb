<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Joining Checklist Master</h3>
    <div class="pull-right">

    <%= link_to 'Assign To Employee', set_checklist_joining_checklist_masters_path,class: 'btn btn-warning btn-xl fa fa-check' %>

    <%= link_to " PDF ", joining_checklist_master_joining_checklist_masters_path(id: @joining_checklist_master.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", joining_checklist_master_joining_checklist_masters_path(id: @joining_checklist_master.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to " Upload", import_xl_joining_checklist_masters_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
     <%= link_to " Template", "/Templates/36.Joining Checklist Master.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
     </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="joining_check_list">
			<%= render 'joining_check_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->