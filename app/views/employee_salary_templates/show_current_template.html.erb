<div class="box box-default">
  <div class="box-body">


      <%= link_to " Excel Report ",current_template_employee_salary_templates_path(emp: @employee_ids, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>

      <table id="example1" class="table table-bordered table-hover table-condensed">
        <thead>
          <tr>
            <th>ID</th>
            <th>Employee Name</th>
          </tr>
        </thead>
        <tbody>
          <% @employee_ids.each do |e| %>
          <% emp = Employee.find_by(id: e) %>
            <tr> 
              <td><%= emp.manual_employee_code %></td>
              <td><%= full_name(emp) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>