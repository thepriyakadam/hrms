<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Question No.</th>
      <th>Question</th>
      <th>Action</th>
 
  <tbody>
    <% @question_masters.each do |question_master| %>
      <tr>
        <td><%= question_master.code %></td>
        <td><%= question_master.name %></td>
        <td>
          <% if question_master.is_confirm == true %>
            <small class= "label label-primary">Confirmed</small>
          <% else %>
            <%= link_to '', edit_question_master_path(question_master),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
            <%= link_to '',  question_master, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-danger btn-xs fa fa-close' %>
            <%= link_to '', is_confirm_question_masters_path(question_master: question_master),data: { confirm: 'Are you sure?'},class: 'btn btn-xs btn-info fa fa-check' %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>