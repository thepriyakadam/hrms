<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Rating Master</h3>
    <div class="pull-right">
    <%= link_to " PDF ", rating_master_ratings_path(id: @rating.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", rating_master_ratings_path(id: @rating.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

        <%= link_to " Upload  ", import_xl_ratings_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
    
    <%= link_to " Template", "/Templates/43.Rating Master.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="rating_list">
			<%= render 'rating_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

