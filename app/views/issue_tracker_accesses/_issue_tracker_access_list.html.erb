<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
   <tr>
    <!--   <th>Name</th> -->
      <th>Role</th>
      <th>Issue Tracker Group</th>
      <th>Issue Tracker Member</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @issue_tracker_accesses.each do |issue_tracker_access| %>
      <tr>
       <!--  <td><%= issue_tracker_access.name %></td> -->
        <td><%= issue_tracker_access.role %></td>
        <td><%= issue_tracker_access.issue_tracker_group.try(:name) %></td>
        <td><%= issue_tracker_access.issue_tracker_member.try(:employee).try(:first_name) %> <%= issue_tracker_access.issue_tracker_member.try(:employee).try(:middle_name) %> <%= issue_tracker_access.issue_tracker_member.try(:employee).try(:last_name) %></td>
        <td><%= issue_tracker_access.status ? "Active" : "Inactive" %></td>
        
        <td>
          <%= link_to '', edit_issue_tracker_access_path(issue_tracker_access),class: 'btn btn-success btn-xs fa fa-edit',remote:true %>
          <%= link_to '',  issue_tracker_access, method: :delete, data: { confirm: 'Are you sure?',remote:true },class: 'btn btn-danger btn-xs fa fa-close' %>
          
      </tr>
    <% end %>
  </tbody>
</table>