<table id="example2" class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>  
      <th>Code</th>
      <th>Description</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <% @rating_masters.each do |rating_master| %>
      <tr>
        <td><%= rating_master.code %></td>
        <td><%= rating_master.description %></td>
        <td>
          <%= link_to '', edit_rating_master_path(rating_master),class: 'btn btn-xs btn-success fa fa-edit',remote:true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>