<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">District</h3>

    <div class="pull-right">
	    <%= link_to " PDF ", district_master_districts_path(id: @district.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
	    
	    <%= link_to " XLS ", district_master_districts_path(id: @district.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>
	    
	    <%= link_to " Upload ", import_xl_districts_path, class: 'fa fa-upload btn btn-xl btn-primary' %>
	    
	     <%= link_to " Template", "/Templates/72.District.xls", :class=>"fa fa-download  btn-xl btn btn-warning" %>
    </div>
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="district_list">
			<%= render 'district_list' %>
		</div>
		
	</div><!-- /.box-body -->
</div><!-- /.box -->