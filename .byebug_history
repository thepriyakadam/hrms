exit
end
@bonus_employees = BonusEmployee.where(employee_id: s.employee_id).group(:employee_id)
@salaryslips.each do |s|
@salaryslips = Salaryslip.where(month: @month.to_s, year: @year.to_s).group(:employee_id)
@year = params[:year]
@month = params[:month]
@salaryslips = Salaryslip.where(month: @month.to_s, year: @year.to_s).group(:employee_id)
@year = params[:year]
@month = params[:month]
exit
@year = params[:year]
@month = params[:month]
