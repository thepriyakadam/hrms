<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Recruiter</h3>
    <div class="pull-right">
    <%= link_to " PDF ", recruiter_master_recruiters_path(id: @recruiter.id, format: "pdf"), class: "btn btn-xl btn-info fa fa-print",:target => "_blank" %>
    
    <%= link_to " XLS ", recruiter_master_recruiters_path(id: @recruiter.id, format: "xls"), class: "btn btn-xl btn-success fa fa-print",:target => "_blank" %>

    <%= link_to "Upload Recruiter", import_xl_recruiters_path, class: 'btn btn-xs btn-primary' %>
    
     <%= link_to "Template", "/Templates/53.Recruiter.xls", :class=>" btn-xs btn btn-warning" %>
     </div>
  </div><!-- /.box-header -->
	<div class="box-body">
		<div id="form">
			<%= render 'form' %>	
		</div>
		
		<div id="recruiter_list">
			<%= render 'recruiter_list' %>
		</div>
	</div><!-- /.box-body -->
</div><!-- /.box -->

