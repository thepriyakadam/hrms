<p id="notice"><%= notice %></p>

<h1>Listing Rating Masters</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rating_masters.each do |rating_master| %>
      <tr>
        <td><%= rating_master.code %></td>
        <td><%= rating_master.description %></td>
        <td><%= link_to 'Show', rating_master %></td>
        <td><%= link_to 'Edit', edit_rating_master_path(rating_master) %></td>
        <td><%= link_to 'Destroy', rating_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating master', new_rating_master_path %>
